	<false/>
	<key>CFBundleURLTypes</key>
	<array>
		<dict>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>sentinela</string>
			</array>
			<key>CFBundleURLName</key>
			<string>com.sentinelaapp.pair</string>
			<key>CFBundleURLRole</key>
			<string>Editor</string>
		</dict>
	</array>
